<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Toplevel" id="mainwindow">
    <property name="background">#5cce76</property>
    <property name="borderwidth">5</property>
    <property name="container">false</property>
    <property name="geometry">1600x1050</property>
    <property name="height">1000</property>
    <property name="highlightbackground">#5cce76</property>
    <property name="maxsize">1600|1200</property>
    <property name="minsize">800|600</property>
    <property name="relief">ridge</property>
    <property name="resizable">both</property>
    <property name="title" translatable="yes">Daniel`s PictureToArray</property>
    <property name="width">400</property>
    <child>
      <object class="ttk.Labelframe" id="mainlabelframe">
        <property name="borderwidth">2</property>
        <property name="height">1000</property>
        <property name="text" translatable="yes">PictureToArray		</property>
        <property name="width">800</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Frame" id="frame3">
            <property name="height">1200</property>
            <property name="padding">20</property>
            <property name="relief">flat</property>
            <property name="width">1600</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">1</property>
              <property name="pady">1</property>
              <property name="propagate">False</property>
              <property name="row">0</property>
            </layout>
            <child>
              <object class="tk.Canvas" id="canva">
                <property name="background">#2c8b98</property>
                <property name="borderwidth">10</property>
                <property name="confine">false</property>
                <property name="height">720</property>
                <property name="relief">groove</property>
                <property name="state">normal</property>
                <property name="takefocus">false</property>
                <property name="width">1530</property>
                <property name="xscrollincrement">10</property>
                <property name="yscrollincrement">10</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">4</property>
                  <property name="ipadx">1</property>
                  <property name="ipady">1</property>
                  <property name="padx">5</property>
                  <property name="pady">2</property>
                  <property name="propagate">False</property>
                  <property name="row">0</property>
                  <property name="sticky">n</property>
                  <property type="row" id="0" name="minsize">0</property>
                  <property type="row" id="0" name="pad">0</property>
                  <property type="row" id="0" name="weight">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="quit_button">
                <property name="command" type="command" cbtype="simple">on_quit_button_clicked</property>
                <property name="compound">none</property>
                <property name="text" translatable="yes">Quit</property>
                <layout manager="grid">
                  <property name="column">3</property>
                  <property name="columnspan">1</property>
                  <property name="ipadx">85</property>
                  <property name="ipady">25</property>
                  <property name="padx">15</property>
                  <property name="propagate">False</property>
                  <property name="row">3</property>
                  <property name="rowspan">2</property>
                  <property name="sticky">e</property>
                  <property type="col" id="2" name="minsize">0</property>
                  <property type="col" id="2" name="weight">0</property>
                  <property type="col" id="3" name="pad">210</property>
                  <property type="row" id="3" name="pad">5</property>
                  <property type="row" id="3" name="weight">0</property>
                  <property type="row" id="4" name="weight">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="frame5">
                <property name="borderwidth">2</property>
                <property name="cursor">arrow</property>
                <property name="height">200</property>
                <property name="relief">raised</property>
                <property name="width">100</property>
                <layout manager="place">
                  <property name="anchor">nw</property>
                  <property name="relwidth">0.22</property>
                  <property name="relx">0.48</property>
                  <property name="rely">0.725</property>
                  <property name="x">0</property>
                  <property name="y">0</property>
                </layout>
                <child>
                  <object class="ttk.Radiobutton" id="radiobutton3">
                    <property name="text" translatable="yes">Disable B ch.</property>
                    <layout manager="pack">
                      <property name="anchor">s</property>
                      <property name="expand">true</property>
                      <property name="fill">y</property>
                      <property name="propagate">False</property>
                      <property name="side">right</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Radiobutton" id="radiobutton4">
                    <property name="text" translatable="yes">Disable G ch.
</property>
                    <layout manager="pack">
                      <property name="anchor">s</property>
                      <property name="expand">true</property>
                      <property name="fill">y</property>
                      <property name="side">right</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Radiobutton" id="radiobutton5">
                    <property name="text" translatable="yes">Disable R ch.</property>
                    <layout manager="pack">
                      <property name="anchor">s</property>
                      <property name="expand">true</property>
                      <property name="fill">y</property>
                      <property name="propagate">False</property>
                      <property name="side">right</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="frame6">
                <property name="height">30</property>
                <property name="padding">10</property>
                <property name="relief">flat</property>
                <property name="width">600</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">5</property>
                  <property name="ipadx">1</property>
                  <property name="propagate">False</property>
                  <property name="row">3</property>
                  <property name="rowspan">1</property>
                  <property type="row" id="3" name="pad">5</property>
                  <property type="row" id="3" name="weight">0</property>
                  <property type="row" id="4" name="weight">1</property>
                </layout>
                <child>
                  <object class="ttk.Button" id="image_button">
                    <property name="command" type="command" cbtype="simple">on_image_button_clicked</property>
                    <property name="text" translatable="yes">Open image</property>
                    <layout manager="pack">
                      <property name="anchor">center</property>
                      <property name="fill">x</property>
                      <property name="ipadx">100</property>
                      <property name="ipady">20</property>
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="button1">
                    <property name="command" type="command" cbtype="simple">on_turn_button_clicked</property>
                    <property name="text" translatable="yes">Turn image</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="ipadx">100</property>
                      <property name="ipady">20</property>
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="channel_button">
                    <property name="command" type="command" cbtype="simple">on_channel_button_clicked</property>
                    <property name="text" translatable="yes">Disable R/G/B channel</property>
                    <layout manager="pack">
                      <property name="ipadx">100</property>
                      <property name="ipady">20</property>
                      <property name="propagate">False</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="button2">
                    <property name="command" type="command" cbtype="simple">on_group_button_clicked</property>
                    <property name="text" translatable="yes">Group pixels</property>
                    <layout manager="pack">
                      <property name="anchor">center</property>
                      <property name="fill">y</property>
                      <property name="ipadx">100</property>
                      <property name="ipady">20</property>
                      <property name="propagate">False</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="textframe">
                <property name="borderwidth">2</property>
                <property name="height">150</property>
                <property name="relief">sunken</property>
                <property name="width">745</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="propagate">True</property>
                  <property name="row">4</property>
                  <property name="rowspan">3</property>
                  <property name="sticky">nw</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Labelframe" id="labelframe1">
            <property name="height">200</property>
            <property name="text" translatable="yes">labelframe1</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Toplevel" id="msgbox">
    <property name="background">#e9e9f8</property>
    <property name="borderwidth">8</property>
    <property name="geometry">480x320</property>
    <property name="height">400</property>
    <property name="maxsize">480|320</property>
    <property name="minsize">480|320</property>
    <property name="relief">groove</property>
    <property name="takefocus">true</property>
    <property name="width">400</property>
    <child>
      <object class="ttk.Frame" id="frame1">
        <property name="height">400</property>
        <property name="width">400</property>
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
